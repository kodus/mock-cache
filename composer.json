{
    "name": "kodus/mock-cache",
    "description": "PSR-16 cache-implementation for integration tests",
    "authors": [
        {
            "name": "Rasmus Schultz",
            "email": "rasc@jfmedier.dk",
            "role": "Developer"
        },
        {
            "name": "Thomas Pedersen",
            "email": "thno@jfmedier.dk",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">= 8.0",
        "psr/simple-cache": "^3"
    },
    "require-dev": {
        "codeception/codeception": "^5",
        "codeception/module-asserts": "^3.0"
    },
    "autoload": {
        "psr-4": {
            "Kodus\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kodus\\Cache\\Test\\": "tests/",
            "Kodus\\Cache\\Test\\Unit\\": "tests/unit/"
        }
    }
}
