{
    "name": "kodus/mock-cache",
    "description": "PSR-16 cache-implementation for integration tests",
    "authors": [
        {
            "name": "Rasmus Schultz",
            "email": "rasc@jfmedier.dk",
            "role": "Developer"
        }
    ],
    "license": "MIT",
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "php": ">= 5.6",
        "psr/simple-cache": "^1"
    },
    "require-dev": {
        "codeception/codeception": "^2",
        "mindplay/simple-cache": "^1"
    },
    "autoload": {
        "psr-4": {
            "Kodus\\Cache\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Kodus\\Cache\\Test\\": "tests/",
            "Kodus\\Cache\\Test\\Integration\\": "tests/integration/"
        }
    }
}
